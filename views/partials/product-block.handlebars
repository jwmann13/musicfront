<div class="col-4">
    <div class="card mt-2">
        <div class="row no-gutters">
            <div class="col-md-6">
                <img src={{dataValues.photo}} class="card-img" alt="...">
            </div>
            <div class="col-md-6">
                <div class="card-body">
                    <h5 class="card-title"><a href="/product/info/{{dataValues.id}}" class="product-link" >{{dataValues.name.toLowerCase}}</a></h5>
                    <p class="card-text">{{dataValues.instrumentFamily}} > {{dataValues.brand}} > {{dataValues.model}}</p>
                    <p>${{dataValues.price}}</p>
                </div>
            </div>
        </div>
    </div>
</div>